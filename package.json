{
    "name": "typewriter",
    "displayName": "typewriter syntax highlighting",
    "description": "highlights typewriter markdown",
     "version": "0.0.1",
    "engines": {
        "vscode": "^1.104.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "iconThemes": [
        {
            "id": "typewriter",
            "label": "Typewriter",
            "path": "./tw-icon-theme.json"
        }
    ],
    "configurationDefaults": {
        "editor.tokenColorCustomizations": {
            "textMateRules": [
                {
                    "scope": "tag.tw",
                    "settings": {
                        "foreground": "#b0ccff"
                    }
                }
            ]
        }
    },
    "languages": [{
        "id": "typewriter",
        "aliases": ["Typewriter", "typewriter"],
        "extensions": [".tw"],
        "configuration": "./language-configuration.json"
    }],
        "grammars": [{
            "language": "typewriter",
            "scopeName": "source.tw",
            "path": "./syntaxes/typewriter.tmLanguage.json"
        }]
    }
}
